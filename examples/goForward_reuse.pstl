goForward p = case p of
    (let xs = proj1 p ;
    case xs of
        (ret p)
        (let bs = proj2 p ;
        let c1 = reset p ;
        let x = proj1 xs ; let xsbis = proj2 xs ;
        let c2 = reset xs ;
        let bsbis = reuse c2 in ctor2 x bs ;
        let r = reuse c1 in ctor1 xsbis bsbis ; ret r))